{
  "expo": {
    "name": "Tour Tour",
    "slug": "tourtour-rn",
    "description": "An app to help touring artists share their favorites spots.",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "comgooglemaps"
        ],
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}